<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"
        android:top="8dp">
        <rotate android:fromDegrees="45">
            <shape>
                <size
                    android:width="45dp"
                    android:height="45dp" />
                <solid android:color="@color/lightGrayLuki"></solid>
                <corners android:radius="5dp"></corners>
            </shape>

        </rotate>
    </item>
</layer-list>